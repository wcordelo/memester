{
  "name": "memester-frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:local": "vite --mode localrpc",
    "dev:host": "vite --host 0.0.0.0",
    "dev:prod": "vite --mode production",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "preview": "vite build && vite preview",
    "format": "prettier --check 'src/**/*.ts*'",
    "format:write": "prettier --write 'src/**/*.ts*'",
    "lint": "eslint 'src/**/*.ts*'",
    "lint:fix": "eslint --fix 'src/**/*.ts*'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts*": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@fontsource/nunito": "^4.5.10",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@memester-xyz/lens-use": "^0.1.1",
    "@mui/icons-material": "^5.10.2",
    "@mui/material": "^5.10.2",
    "@rainbow-me/rainbowkit": "^0.8.0",
    "@reactour/tour": "^3.3.0",
    "@types/react-color": "^3.0.6",
    "@types/styled-components": "^5.1.26",
    "antd": "^5.2.3",
    "axios": "^0.27.2",
    "buffer": "^6.0.3",
    "dayjs": "^1.11.7",
    "ethers": "^5.7.2",
    "events": "^3.3.0",
    "graphql": "^16.6.0",
    "history": "^5.3.0",
    "html2canvas": "^1.4.1",
    "ipfs-http-client": "^59.0.0",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "process": "^0.11.10",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "react-helmet-async": "^1.3.0",
    "react-hot-toast": "^2.4.0",
    "react-markdown": "^8.0.3",
    "react-moveable": "^0.38.4",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scroll-parallax": "^3.3.1",
    "react-selecto": "^1.19.1",
    "react-use": "^17.4.0",
    "styled-components": "^5.3.5",
    "use-local-storage-state": "^18.1.1",
    "util": "^0.12.4",
    "uuid": "^9.0.0",
    "wagmi": "^0.8.10",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.9.4",
    "vite": "^3.0.9",
    "vite-plugin-rewrite-all": "^1.0.0",
    "vite-plugin-svgr": "^2.2.1"
  }
}
